            "spi_layout": "A00000:FFFFFF normal\n400000:9EFFFF fallback",
            "programmer_type": "internal",
            "flashromExtraArgs": [
              "-i",
              "normal",
              "-i",
              "fallback",
              "--noverify-all"
            ],
            "chip": [
              "MX25L12805D",
              "N25Q128..3E"
            ]
          }
        }
      ],
      "verify": {
        "commandType": "flashrom",
        "flashromArgs": {
          "programmer_type": "internal",
          "flashromExtraArgs": [
            "-i",
