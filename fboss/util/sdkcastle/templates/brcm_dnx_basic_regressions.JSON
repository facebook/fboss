{
  "testSdkVersion": "brcm-sai/12.2.0.0_odp",
  "runMode": "list-test-runner-cmds-only",
  "buildMode": "opt-asan",
  "testRunnerMode": "meta-internal",
  "testOutDirPath": "~/mylogs/",
  "testSpecs": {
    "agentTests": [
      {
        "testName": "jericho3AgentTests",
        "commonTestSpec": {
          "testTeam": "sai_agent_test",
          "asicTestOptions": {
            "jericho3": {
              "asicTestName": "j3",
              "bassetQuery": {
                "devicePool": "fboss.autotest",
                "asic": "jericho3"
              },
              "numJobs": 1,
              "regex": "addRemoveNeighbor"
            }
          }
        }
      },
      {
        "testName": "ramon3AgentTests",
        "commonTestSpec": {
          "testTeam": "sai_agent_test",
          "asicTestOptions": {
            "ramon3": {
              "asicTestName": "r3",
              "bassetQuery": {
                "devicePool": "fboss.autotest",
                "asic": "ramon3"
              },
              "numJobs": 1,
              "regex": "collectStats"
            }
          }
        }
      }
    ]
  }
}
